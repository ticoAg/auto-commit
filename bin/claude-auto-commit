#!/usr/bin/env node
import path from "path";
import { fileURLToPath, pathToFileURL } from "url";

const __filename = fileURLToPath(import.meta.url);
const __dirname = path.dirname(__filename);

const warningKey = "AUTO_COMMIT_ALIAS_WARNED";
if (!process.env[warningKey]) {
	console.warn("⚠️  'claude-auto-commit' 已更名为 'auto-commit'。该别名将在后续版本中移除，请尽快更新脚本和习惯用法。");
}

const target = path.join(__dirname, "auto-commit");
await import(pathToFileURL(target).href);
